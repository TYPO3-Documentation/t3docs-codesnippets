{
	"name": "t3docs/codesnippets",
	"description" : "TYPO3 CMS Base Distribution",
	"license": "GPL-2.0-or-later",
	"config": {
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		},
		"sort-packages": true
	},
    "repositories": {
        "0_local_packages": {
            "type": "path",
            "url": "local_packages/*"
        },
        "1c_git": {
            "type": "vcs",
            "url": "https://github.com/TYPO3-Documentation/blog_example.git"
        },
        "1d_git": {
            "type": "vcs",
            "url": "https://github.com/TYPO3-Documentation/documentation.git"
        }
    },
	"require": {
		"friendsoftypo3/blog-example": "dev-12.0-dev",
		"haydenpierce/class-finder": "^0.4.3",
		"t3docs/documentation": "@dev",
		"t3docs/examples": "dev-main",
		"t3docs/restructured-api-tools": "@dev",
		"t3docs/site-package": "dev-main",
		"ttn/tea": "12.0-dev-dev",
		"typo3-documentation-team/speeddemo": "^11.4",
		"typo3/cms-adminpanel": "main-dev",
		"typo3/cms-backend": "main-dev",
		"typo3/cms-belog": "main-dev",
		"typo3/cms-beuser": "main-dev",
		"typo3/cms-core": "main-dev",
		"typo3/cms-dashboard": "main-dev",
		"typo3/cms-extbase": "main-dev",
		"typo3/cms-extensionmanager": "main-dev",
		"typo3/cms-felogin": "main-dev",
		"typo3/cms-filelist": "main-dev",
		"typo3/cms-filemetadata": "main-dev",
		"typo3/cms-fluid": "main-dev",
		"typo3/cms-fluid-styled-content": "main-dev",
		"typo3/cms-form": "main-dev",
		"typo3/cms-frontend": "main-dev",
		"typo3/cms-impexp": "main-dev",
		"typo3/cms-indexed-search": "main-dev",
		"typo3/cms-info": "main-dev",
		"typo3/cms-install": "main-dev",
		"typo3/cms-linkvalidator": "main-dev",
		"typo3/cms-lowlevel": "main-dev",
